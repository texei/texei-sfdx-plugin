{
  "name": "texei-sfdx-plugin",
  "description": "Texeï's plugin for sfdx",
  "version": "1.9.3",
  "author": "Texeï",
  "bugs": "https://github.com/https://github.com/texei/texei-sfdx-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^3.1.0",
    "@salesforce/core": "^2.1.5",
    "bl": "^5.0.0",
    "child-process-promise": "^2.2.1",
    "npm-check-updates": "^11.5.1",
    "puppeteer": "^9.0.0",
    "tslib": "^2.2.0",
    "unzipper": "^0.10.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "2.1.0",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/node": "^14",
    "chai": "^4",
    "globby": "^11.0.3",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/https://github.com/texei/texei-sfdx-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "texei": {
        "description": "Texeï's plugin for sfdx"
      },
      "texei:org": {
        "description": "Commands to manage org"
      }
    }
  },
  "repository": "https://github.com/texei/texei-sfdx-plugin",
  "scripts": {
    "postpack": "rimraf -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
